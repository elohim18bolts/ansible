{% if write_config_mode is defined %}
write-kube-config-mode: "{{ kube_config_mode }}"
{% endif %}
cluster-init: {{ cluster_init }}
tls-san:
{% for i in tls_san_list %}
  - "{{ i }}"
{% endfor %}
cluster-cdir: "{{ cluster_cdir }}"
disable:
{% for i in disable %}
  - "{{ i }}"
{% endfor %}
{% if dns_servers is defined %}
resolv-conf: "/etc/k3s_resolv.conf"
{% endif %}

